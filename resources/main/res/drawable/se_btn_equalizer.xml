<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:bottom="2px"
        android:left="2px"
        android:right="2px"
        android:top="2px">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#229f63" />
        </shape>
    </item>
    <item>
        <drawable
            class="com.xiaopeng.xui.drawable.ripple.XRippleDrawable"
            app:rippleRadius="@dimen/x_btn_real_radius"
            app:rippleSupportScale="true" />
    </item>
</layer-list>

