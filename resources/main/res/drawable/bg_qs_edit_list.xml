<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <solid android:color="@color/qs_panel_edit_list_bg_drag_color" />
            <stroke android:width="1dp" android:color="@color/qs_panel_edit_list_bg_drag_line_color"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <solid android:color="@color/edit_panel_item_color" />
        </shape>
    </item>
</selector>

